<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-4 col-sm-offset-4">
      <div class="page-header">
        <h1>My Profile:
          <small>Here you can edit your data</small>
        </h1>
      </div>
    </div>
    <div class="col-xs-12 col-sm-4 col-sm-offset-4">
      <form action="/user/<%=session.id%>" method="POST">
        <div class="form-group">
          <div class="input-group">
            <span class="input-group-addon">
              <i class="fa fa-envelope"></i>
            </span>
            <input type="text" value="<%=session.name%>" name="name" class="form-control" placeholder="Name">
          </div>
        </div>
        <div class="form-group">
          <div class="input-group">
            <span class="input-group-addon">
              <i class="fa fa-envelope"></i>
            </span>
            <input type="email" value="<%=session.email%>" name="email" class="form-control" placeholder="Email">
          </div>
        </div>
        <div class="form-group">
          <p><strong>Paste here the link address:</strong></p>
          <div class="input-group">
            <span class="input-group-addon">
              <i class="fa fa-envelope"></i>
            </span>
            <input type="text" value="<%=session.imgUrl%>" id="inpImgUrl" name="imageUrl" class="form-control" placeholder="Image URL">
          </div>
        </div>
        <img src="" id="imgUrl" alt="" height="100px">
        <br>
        <div class="form-actions">
            <br>
          <button type="submit" class="btn btn-block btn-primary">Update</button>
        </div>
      </form>
    </div>
  </div>
</div>
<script>
  var i = 0;
  $("#inpImgUrl")
    .mouseover(function () {
      // $(this).attr("src", $(this).data("hover_src"));
      // $("imgUrl", this).text("mouse over");
      $("#imgUrl").attr("src",$("#inpImgUrl").val())
    })
</script>