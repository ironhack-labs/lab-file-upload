<div>
    <h2>Hi <%= username %> !</h2>

    <h2>
        <%= user.username %> posts</h2>

    <% posts.forEach(function(post) { %>
        <div>
            <p>
                <%= post.content %>
            </p>
            <p>
                <%= moment(post.created_at).format('LL, LTS') %>
            </p>
        </div>
        <% }) %>
</div>