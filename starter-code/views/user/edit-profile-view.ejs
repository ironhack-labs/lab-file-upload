<h2> Edit your profile here</h2>


form method="post" action="/celebrities/<%= celebrity._id%>">
  <label for="user-name-input">Product Name </label>
  <input  id="user-name-input" type="text"
      name="userName" value="<%= user.name %>">
  <br>

  <label for="celebrity-ocupation-input">Ocupation </label>
  <input  id="celebrity-ocupation-input" type="text"
      name="celebrityOcupation" value="<%= celebrity.ocupation %>">
  <br>

  <label for="celebrity-phrase-input">Catch Phrase </label>
  <input  id="celebrity-phrase-input" type="text"
      name="celebritycatchPhrase" value="<%= celebrity.catchPhrase %>">
  <br>

  <label for="celebrity-image-url-input">Profile Image </label>
  <input  id="celebrity-image-url-input" type="text"
      name="celebrityImageUrl" value="<%= celebrity.profileImage %>">
  <br>


  <button> Save </button>
</form>
